{
	"name": "counties",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"prod": "vite build && vite preview",
		"clean": "biome check --write && tsc",
		"collect:temps": "node scripts/base/temperatures.js && npm run clean",
		"collect:votes": "node scripts/base/elections.js && npm run clean",
		"collect:housing": "node scripts/base/housing.js && npm run clean",
		"collect:age": "node scripts/base/ages.js && npm run clean",
		"collect:rents": "node scripts/base/rents.js && npm run clean",
		"collect:population": "node scripts/base/population.js && npm run clean",
		"collect": "npm run collect:temps && npm run collect:votes && npm run collect:housing && npm run collect:age && npm run collect:rents && npm run collect:population && npm run clean",
		"fill": "node scripts/fill/fillEstimates.js && npm run clean",
		"combine": "node scripts/final/combineBase.js && node scripts/final/fillMissing.js && npm run clean",
		"data:generate": "npm run collect && npm run fill && npm run combine && npm run clean",
		"data:reset": "rm -rf data/base data/fill data/final && npm run data:generate && npm run clean"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.7",
		"@heroicons/react": "^2.2.0",
		"axios": "^1.1.3",
		"d3": "^7.6.1",
		"date-fns": "^4.1.0",
		"just-standard-deviation": "^2.1.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tar": "^7.5.1",
		"topojson-client": "^3.1.0",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/d3": "^7.4.0",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"@types/topojson-client": "^3.1.1",
		"@vitejs/plugin-react": "^5.0.2",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.18",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.9.2",
		"vite": "^7.1.5"
	}
}
